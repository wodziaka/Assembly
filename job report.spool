******************************************************************      00060000
* CSCI 360-2             LAB PROGRAM 3                 FALL 2024 *      00070000
*              DOC BOX GOES HERE (REQUIRED!)                     *      00080000
* NAME: ANNIKA WODZIAK                                           *      00090000
* DATE: 11/29/2024                                               *      00100000
*                                                                *
* In this lab, payroll data is given and a table must be         *
* created displaying the given data alongside calculated         *
* information such as state and federal withholding using        *
* packed decimals. Totals are also calculated.                   *
*                                                                *      00110000
* REGISTER USAGE:                                                *      00120000
*   R2 - HOLDS THE HEAD OF THE TABLE                             *      00130000
*   R3 - HOLDS THE END OF THE TABLE                              *      00140000
*   R5 - ALSO HOLDS THE TABLE HEAD                               *
*   R6 - $ITABLE DSECT BASE REGISTER                             *
*                                                                *
*                                                                *      00150000
******************************************************************      00160000
* DSECTs
$TABLROW DSECT                PROGRAM STORAGE ROW FORMAT(EMPNTRY)
$PAYRTE  DS    PL3            HOURLY PAY RATE
$HOURS   DS    PL3            HOURS WORKED IN PAY PERIOD
$DEDUCT  DS    PL3            DEDUCTION AMOUNT
$BONUS   DS    PL3            BONUS AMOUNT
$GROSS   DS    PL6            GROSS PAY
$NETPAY  DS    PL8            NET PAY
$PFEDWH  DS    PL6            FEDERAL WITHHOLDING
$PSTWH   DS    PL6            STATE WITHHOLDING
$EMPID   DS    CL8            EMPLOYEE ID
$EMPNME  DS    CL25           EMPLOYEE NAME
*********************************************************************
* Main program
PAYROLL  CSECT
         STM   14,12,12(13)            SAVE REGISTERS
         LR    12,15                   COPY CSECT ADDR INTO R12
         USING PAYROLL,12,11           ESTABLISH 12 & 11 AS BASE REG
         LA    11,4095(,12)            SECOND BASE REG
         LA    11,1(,11)
*
         LA    14,MAINSAVE             POINTS TO SAVE AREA
         ST    13,4(,14)               STORE ADDR OF CALLER SAVE AREA
         ST    14,8(,13)               STORE ADDR OF CSECT SAVE AREA
         LR    13,14                   POINT R13 AT CSECT SAVE AREA
*
* Call BUILDTBL
*
         LA    1,BTPARMS               LOAD R1 WITH BTPARMS
         L     15,=V(BUILDTBL)         CALL BUILDTBL
         BALR  14,15
*
* Call PROCTBL
         LA    1,PTPARMS               LOAD R1 WITH PTPARMS
         L     15,=V(PROCTBL)          CALL PROCTBL
         BALR  14,15
*
* CALL PRNTTABL
         LA    1,PRNPARMS              LOAD R1 WITH PRNPARMS
         L     15,=V(PRNTTABL)         CALL PRNTTABL
         BALR  14,15
*
* CALL PRNTSUMR
         LA    1,PRSMPRMS              LOAD R1 WITH PRSMPARMS
         L     15,=V(PRNTSUMR)         CALL PRNTSUMR
         BALR  14,15
*
* Rest of the main program logic goes here
*
* ==== Exit linkage ============
PRGEXIT  LA    15,0                    R15 = RETURN CODE OF 0
         L     13,4(,13)               POINT R13 TO CALLER'S SAVE AREA
         L     14,12(,13)              RESTORE REGISTER 14
         LM    0,12,20(13)             RESTORE R0 THRU R12
         BR    14                      RETURN TO CALLER
*
         DROP  12,11
         LTORG
MAINSAVE DC    18F'-1'                 PROGRAM SAVE AREA
BTPARMS  DC    A(PFWHPCT,PSWHPCT,EMPTBL,TBLEND,PEMPCTR)
PTPARMS  DC    A(PFWHPCT,PSWHPCT,EMPTBL,PEMPCTR,SUMSTRCT)
PRNPARMS DC    A(EMPTBL,PEMPCTR)
PRSMPRMS DC    A(SUMSTRCT)
PFWHPCT  DS    PL4'0'
PSWHPCT  DS    PL4'0'
         DS    0F
EMPTBL   DS    117CL80
TBLEND   DS    0C
PEMPCTR  DC    PL3'0'                  EMPLOYEE COUNTER
SUMSTRCT DC    PL6'0'                  TOTAL GROSS PAY
         DC    PL8'0'                  AVG GROSS PAY
         DC    PL6'0'                  TOTAL FED WH
         DC    PL10'0'                 AVG FED WH
         DC    PL6'0'                  TOTAL STATE WH
         DC    PL10'0'                 AVG STATE WH
         DC    PL8'0'                  TOTAL NET PAY
         DC    PL8'0'                  AVG NET PAY
*
**********************************************************************
* BUILDTBL subprogram                                                *
**********************************************************************
* IRECORD DSECT
$IRECORD DSECT                INPUT BUFFER FORMAT(EMPSALE)
$IEMPID  DS    CL8            (INPUT)EMPLOYEE ID
$IPAYRTE DS    ZL5            (INPUT)HOURLY PAY RATE
$IHOURS  DS    ZL5            (INPUT)HOURS WORKED IN PAY PERIOD
$IDEDUCT DS    ZL5            (INPUT)DEDUCTION AMOUNT
$IBONUS  DS    ZL5            (INPUT)BONUS AMOUNT
$IEMPNME DS    CL25           (INPUT)EMPLOYEE NAME
$IGROSS  DS    PL5            (INPUT)GROSS PAY
**********************************************************************
BUILDTBL CSECT
         STM   14,12,12(13)      SAVE REGISTERS
         LR    12,15             SET UP BASE REGISTER
         USING BUILDTBL,12       ESTABLISH ADDRESSABILITY
         LA    14,BLDSAVE        R14 POINTS TO THIS CSECT'S SAVE AREA
         ST    13,4(,14)         STORE ADDRESS OF CALLER SAVE AREA
         ST    14,8(,13)         STORE ADDRESS OF CSECT SAVE AREA
         LR    13,14
*
         LA    8,RECORD          LOAD R8 WITH RECORD
         LM    2,6,0(1)          LOAD PARAMETER ADDRESSES
*
         USING $IRECORD,8              USING $IRECORD DSECT
*
         XREAD RECORD,12
         PACK  0(4,2),RECORD(6)        PACK PFWHPCT(4),RECORD(6)
         PACK  0(4,3),RECORD+6(6)      PACK PSWHPCT(4),RECORD+6(6)
*        XDUMP
         XREAD RECORD,80               GET FIRST EMPLOYEE RECORD
BLDLOOP  BNZ   ENDBLD                  EXIT TABLE BUILD LOOP IF EOF
         USING $TABLROW,4              ADDRESSABILITY FOR TABLE
         MVC   $EMPNME(25),$IEMPNME    COPY EMPLOYEE NAME
         MVC   $EMPID(8),$IEMPID       COPY EMPLOYEE ID
         PACK  $PAYRTE(3),$IPAYRTE(5)  CONVERT & STORE PAY RATE
         PACK  $HOURS(3),$IHOURS(5)    CONVERT & STORE HOURS
         PACK  $DEDUCT(3),$IDEDUCT(5)  CONVERT & STORE DEDUCTIONS
         PACK  $BONUS(3),$IBONUS(5)    CONVERT & STORE BONUS
         PACK  $GROSS(3),$IGROSS(5)    CONVERT & STORE GROSS
         AP    0(3,6),=PL1'1'
*
         LA    4,80(,4)                R2 = ADDR OF NEXT ROW
         XREAD RECORD,80               GET NEXT RECORD
         B     BLDLOOP                 BRANCH TO TOP
ENDBLD   DS    0H                      END LOOP
**********************************************************************
         LA    15,0                    R15 = RETURN CODE OF 0
         L     13,4(,13)               POINT R13 TO CALLER SAVE AREA
         L     14,12(,13)              RESTORE REGISTER 14
         LM    0,12,20(13)             RESTORE REMAINING REGISTERS
         BR    14                      RETURN TO CALLER
*
         LTORG
BLDSAVE  DC    18F'-1'                 PROGRAM SAVE AREA
FEDPCT   DS    PL4                     FED WH PERCENTAGE
STPCT    DS    PL4                     ST WH PERCENTAGE
RECORD   DS    CL80                    INPUT RECORDS
**********************************************************************
*  PAYROLL COMPUTATION LOOP DETAILS                                  *
**********************************************************************
$SUMMARY DSECT
$PTGRSS  DS    PL6                     TOTAL GROSS PAY
$PAVGRSS DS    PL10                    AVERAGE GROSS PAY
$PTFWH   DS    PL8                     TOTAL FEDERAL WITHHOLDING
$PAFWH   DS    PL10                    AVERAGE FEDERAL WITHHOLDING
$PTSWH   DS    PL6                     TOTAL STATE WITHHOLDING
$PASWH   DS    PL10                    AVERAGE STATE WITHHOLDING
$PTNET   DS    PL8                     TOTAL NET PAY
$PANET   DS    PL10                    AVERAGE NET PAY
**********************************************************************
PROCTBL CSECT
         STM   14,12,12(13)            SAVE REGISTERS
         LR    12,15                   COPY CSECT ADDR INTO R12
         USING PROCTBL,12              ESTABLISH R12 AS BASE REG
         LA    14,PRCSAVE              R14 POINT TO CSECT SAVE AREA
         ST    14,8(,13)               STORE CSECT SAVE AREA ADDR
         ST    13,4(,14)               STORE CALLER SAVE AREA
         LR    13,14                   R13 POINT AT CSECT SAVE AREA
*
         LM    2,6,0(1)                LOAD PARAM ADDRESSES
         USING $TABLROW,4              USING $TABLROW DSECT
         USING $SUMMARY,6              USING $SUMMARY DSECT
         ST    5,CAPARMS+4             STORE EMPCTR @ CAPARM 2ND PARM
**********************************************************************
PROCLOOP CP    EMPCTR(3),0(3,5)             CP EMPCTR TO PEMPCTR
         BNL   ENDPROC                           B TO ENDPROC
         ZAP   PEMPGPAY(6),$PAYRTE(3)            ZAP $PAYRTE TO PARAM
         MP    PEMPGPAY(6),$HOURS(3)             PAYRTE * HOURS
         SRP   PEMPGPAY,64-2,5                   SHIFT AND ROUND
         SP    PEMPGPAY(6),$DEDUCT(3)            SUBTRACT DEDUCTION
         AP    PEMPGPAY(6),$BONUS(3)             ADD BONUS
         ZAP   $GROSS(6),PEMPGPAY(6)             STORE RES IN $GROSS
**********************************************************************
         ST    2,CNPPARMS+8            ST PFWHPCT @ CNPPARMS 3RD PARM
         ST    3,CNPPARMS+16           ST PSWHPCT @ CNPPARMS 5TH PARM
         LA    8,CNPPARMS              LOAD CNPPARMS INTO R8
         L     15,=V(CALCNPAY)         CALL CALCNPAY
         BALR  14,15
         ZAP   $NETPAY(8),PEMPNPAY(8)       STORE NET PAY
         ZAP   $PFEDWH(6),PFEDWITH(6)       STORE FED WH
         SRP   $PFEDWH,64-3,5               SHIFT & ROUND
         SRP   $PFEDWH,64+3,5               SHIFT & ROUND
         ZAP   $PSTWH(6),PSTWITH(6)         STORE RES IN $PSTWH
         SRP   $PSTWH,64-3,5                SHIFT & ROUND
         SRP   $PSTWH,64+3,5                SHIFT & ROUND
         AP    EMPCTR(3),=PL1'1'            ADD 1 TO EMPCTR
*
* TOTAL GROSS
         AP    $PTGRSS(6),$GROSS(6)    ADD $GROSS TO TOTAL COUNT
*
* TOTAL FEDWH
         AP    PFEDWHTO(6),$PFEDWH(6)  ADD $PFEDWH TO TOTAL COUNT
*
* TOTAL STWH
         AP    PSTWHTO(6),$PSTWH(6)    ADD $PSTWH TO TOTAL COUNT
*
* TOTAL NETPAY
         AP    PNETTO(8),$NETPAY(8)    ADD $NETPAY TO TOTAL COUNT
*
         LA    4,80(,4)                LOAD NEXT LINE
         B     PROCLOOP                BRANCH BACK TO PROCLOOP
ENDPROC  DS    0H                      END LOOP
**********************************************************************
         USING $SUMMARY,6              USING $SUMMARY DSECT
*
* GROSS TOTAL & AVG
         ZAP   PTOTAL(10),$PTGRSS(6)   ZAP TOTAL INTO PARAM TOTAL
         LA    1,CAPARMS               LOAD R1 WITH CAPARMS
         L     15,=V(CALCAVG)          CALL CALCAVG
         BALR  14,15
         ZAP   $PAVGRSS(10),PAVG(8)    ZAP AVG PARAM INTO DSECT AVG
*
* FED WH TOTAL & AVG
         ZAP   PTOTAL(10),PFEDWHTO(6)  ZAP TOTAL INTO PARAM TOTAL
         LA    1,CAPARMS               LOAD R1 WITH CAPARMS
         L     15,=V(CALCAVG)          CALL CALCAVG
         BALR  14,15
         ZAP   $PAFWH(10),PAVG(8)      ZAP AVG PARAM INTO DSECT AVG
         ZAP   $PTFWH(8),PFEDWHTO(6)   ZAP TEMP TOT INTO DSECT TOT
*
* ST WH TOTAL & AVG
         ZAP   PTOTAL(10),PSTWHTO(6)   ZAP TOTAL INTO PARAM TOTAL
         LA    1,CAPARMS               LOAD R1 WITH CAPARMS
         L     15,=V(CALCAVG)          CALL CALCAVG
         BALR  14,15
         ZAP   $PASWH(10),PAVG(8)      ZAP AVG PARAM INTO DSECT AVG
         SRP   $PASWH,64+1,5           SHIFT AND ROUND
         ZAP   $PTSWH(6),PSTWHTO(6)    ZAP TEMP TOT INTO DSECT TOT
*
* NETPAY TOTAL & AVG
         ZAP   PTOTAL(10),PNETTO(8)    ZAP TOTAL INTO PARAM TOTAL
         LA    1,CAPARMS               LOAD R1 WITH CAPARMS
         L     15,=V(CALCAVG)          CALL CALCAVG
         BALR  14,15
         ZAP   $PANET(10),PAVG(8)      ZAP AVG PARAM INTO DSECT AVG
         ZAP   $PTNET(8),PNETTO(8)     ZAP TEMP TOT INTO DSECT TOT
*
         LA    15,0
         L     13,4(,13)       POINT R13 TO CALLER SAVE AREA
         L     14,12(,13)      RESTORE REGISTER 14
         LM    0,12,20(13)     RESTORE REMAINING REGISTERS
         BR    14              RETURN TO CALLER
*
         LTORG
PRCSAVE  DC    18F'-1'
CNPPARMS DC    A(PEMPGPAY,PEMPNPAY,0,PFEDWITH,0,PSTWITH)
CAPARMS  DC    A(PTOTAL,0,PAVG)
PTOTAL   DC    PL10'0'
PAVG     DC    PL8'0'
PEMPGPAY DC    PL6'0'
PEMPNPAY DC    PL8'0'
PFEDWITH DC    PL6'0'
PSTWITH  DC    PL6'0'
EMPCTR   DC    PL3'0'
PGROSSTO DC    PL6'0'
PSTWHTO  DC    PL6'0'
PNETTO   DC    PL8'0'
PFEDWHTO DC    PL6'0'
**********************************************************************
* CALCULATED EMPLOYEE DETAILS                                        *
**********************************************************************
CALCNPAY CSECT
         STM   14,12,12(13)            SAVE REGS IN CALLER SAVE AREA
         LR    12,15                   COPY CSECT ADDRESS INTO R12
         USING CALCNPAY,12             ESTABLISH R12 AS BASE REG
         LA    14,CALCSAVE             R14 POINTS @ CSECT SAVE AREA
         ST    14,8(,13)               STORE CSECT SAVE AREA ADDR
         ST    13,4(,14)               STORE CALLER SAVE AREA ADDR
         LR    13,14                   POINT R13 AT CSECT SAVE AREA
*
         LM    2,7,0(8)                DEREF PARAM LIST
         USING $TABLROW,4              USING $TABLROW DSECT
*
* FEDERAL WITHHOLD
*
         ZAP   PTEMP(10),0(6,2)        LOAD GROSS PAY INTO PTEMP
         MP    PTEMP(10),0(4,4)        MULTIPLY BY PFWHPCT
         SRP   PTEMP(10),64-2,5        SHIFT AND ROUND
         ZAP   0(6,5),PTEMP(10)        LOAD INTO PFEDWITH
*
* STATE WITHHOLD
*
         ZAP   PTEMP(10),0(6,2)        LOAD GROSS PAY INTO PTEMP
         MP    PTEMP(10),0(4,6)        MULTIPLY BY PSWHPCT
         SRP   PTEMP(10),64-2,5        SHIFT AND ROUND
         ZAP   0(6,7),PTEMP(10)        LOAD INTO PSTWITH
*
* NET PAY
*
         ZAP   PTEMP(10),0(6,2)        LOAD GROSS PAY INTO PTEMP
         SRP   PTEMP(10),64+3,5        SHIFT AND ROUND
         SP    PTEMP(10),0(6,5)        SUBTRACT FED WH
         SP    PTEMP(10),0(6,7)        SUBTRACT STATE WH
         SRP   PTEMP(10),64-2,9        SHIFT AND ROUND
         SRP   PTEMP(10),64+3,5        SHIFT AND ROUND
         ZAP   0(8,3),PTEMP(10)        STORE IN NET PAY PARAM
**********************************************************************
         L     13,4(,13)       POINT R13 @ CALLER SAVE AREA
         L     14,12(,13)      RESTORE REGISTER 14
         LM    0,12,20(13)     RESTORE REMAINING REGISTERS
         BR    14              RETURN TO CALLER
*
         LTORG
CALCSAVE DC    18F'-1'         CALCNPAY SAVE AREA
PTEMP    DS    PL10
**********************************************************************
*  CALCULATED AVERAGES AND TOTALS                                    *
**********************************************************************
CALCAVG  CSECT
         STM   14,12,12(13)            SAVE REGS IN CALLER SAVE AREA
         LR    12,15                   COPY CSECT ADDR INTO R12
         USING CALCAVG,12              ESTABLISH R12 AS BASE REG
         LA    14,AVGSAVE              R14 POINTS @ CSECT'S SAVE AREA
         ST    14,8(,13)               STORE CSECT'S SAVE AREA ADDR
         ST    13,4(,14)               STORE CALLER SAVE AREA ADDR
         LR    13,14                   POINT R13 AT CSECT SAVE AREA
*
         LM    2,4,0(1)                DEREF PARAM LIST
*********************************************************************
* AVGS                                                              *
*********************************************************************
         ZAP   0(10,4),0(10,2)          ZAP TOTAL INTO AVG PARAM
         DP    0(10,4),=P'117'         DIVIDE TOTAL BY EMP COUNT
*********************************************************************
         L     13,4(,13)       POINT R13 @ CALLER SAVE AREA
         L     14,12(,13)      RESTORE REGISTER 14
         LM    0,12,20(13)     RESTORE REMAINING REGISTERS
         BR    14              RETURN TO CALLER
*
         LTORG
AVGSAVE  DC    18F'-1'
**********************************************************************
*  PRINTING TABLE DETAILS                                            *
**********************************************************************
PRNTTABL CSECT
         STM   14,12,12(13)    SAVE REGS IN CALLER SAVE AREA
         LR    12,15           COPY CSECT ADDR INTO R12
         USING PRNTTABL,12     ESTABLISH R12 AS BASE REG
         LA    14,PRNTSAVE     R14 POINTS TO CSECT SAVE AREA
         ST    14,8(,13)       STORE CSECT'S SAVE AREA ADDR
         ST    13,4(,14)       STORE CALLER SAVE AREA ADDR
         LR    13,14           POINT R13 AT CSECT SAVE AREA
*
         LM    2,3,0(1)        DEREF PARAM LIST
*
         USING $TABLROW,2      USING $TABLROW DSECT
         USING $IRECORD,4      USING $IRECORD DSECT
*
* PRINT PAGE HEADER
*
         MVC   PNUM(4),=X'40202120'   FORMAT PAGE NUM
         ED    PNUM(4),PPNUM          MOVE VALUE TO PRINTABLE
         L     10,=F'0'               CHK
         L     11,=F'0'               CHK
         LR    5,2            R5 = ADDR OF FIRST UNFILLED ROW
*
*  REPORT GENERATION LOOP DETAILS
*
PHEAD    DS    0H
         MVI   SPACE,C' '
         MVC   SPACE+1(133),SPACE
         XPRNT SPACE,133               PRINT SPACE BEFORE TABLE
         AP    PPNUM(2),=PL1'1'        ADD 1 TO PAGENUM
         MVC   PNUM(4),=X'40202120'    FORMAT PAGE NUMBER
         ED    PNUM(4),PPNUM           PRINTABLE CHAR
         XPRNT HEAD,133                PRINT TABLE HEADER
         XPRNT TITLE,133               PRINT TITLE
         MVI   LINE,C'='               ARRANGE FORMATTING VALUE
         MVC   LINE+1(133),LINE
         XPRNT OUTBUFF                 PRINT FORMATTING LINE
         XPRNT HEAD2,133               PRINT COLUMN SUBHEADERS
         XPRNT HEAD3,133               PRINT COLUMN HEADERS
         MVI   LINE2,C'-'              ARRANGE FORMATTING VALUE
         MVC   LINE2+1(133),LINE2
         XPRNT OUTBUFF2                PRINT FORMATTING LINE
         L     10,=F'0'                Reset line counter
*
REPLOOP  DS    0H
         USING $TABLROW,2
         USING $IRECORD,4
         MVC   OEMPNME(25),$EMPNME     COPY EMPLOYEE NAME TO PRINT BUFF
         MVC   OEMPID(8),$EMPID        COPY EMPLOYEE ID TO PRINT BUFF
         MVC   OPAYRTE(7),=X'402020204B2020'     FORMAT
         LA    1,OPAYRTE+6
         EDMK  OPAYRTE(7),$PAYRTE                PRINTABLE CHAR
         MVC   OHOURS(7),=X'402020204B2020'      FORMAT
         LA    1,OHOURS+6
         EDMK  OHOURS(7),$HOURS                  PRINTABLE CHAR
         MVC   OGROSS(15),=X'402020206B2020206B2020204B2020'    FORMAT
         LA    1,OGROSS+6
         EDMK  OGROSS(15),$GROSS                 PRINTABLE CHAR
         LA    1,ONETPAY+6
         MVC   ONETPAY(15),=X'402020206B2020206B2020204B2020'   FORMAT
         EDMK  ONETPAY(15),$NETPAY               PRINTABLE CHAR
         MVC   OFEDWH(11),=X'402020206B2020204B2020'     FORMAT
         LA    1,OFEDWH+6
         EDMK  OFEDWH(11),$PFEDWH                PRINTABLE CHAR
         MVC   OSTWH(11),=X'402020206B2020204B2020'      FORMAT
         LA    1,OSTWH+6
         EDMK  OSTWH(11),$PSTWH                  PRINTABLE CHAR
*
         XPRNT DETAIL,133              PRINT EMPLOYEE DETAIL RECORD
